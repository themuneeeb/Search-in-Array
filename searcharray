#include <iostream>
using namespace std;
int main()
{
	int a[5] = { 23,45,6,21,5,};
	int search,i;
	bool found;
	cout << " Enter the number you want to search : " << endl;
	cin >> search;
	for ( i = 0; i < 5; i++)
	{
		if (a[i] == search)
		{
			found = true;
			cout << "present " << endl;
			break;

		}
		else
		{
			found = false;
			
		}
		
	}
	if (a[i] != search)
		cout << " Not present";


}
